// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model HistoryEvent {
  id String @id @default(uuid())
  time String

  events String
  eventsMarkup String
  eventIndex Int
  createdAt DateTime @default(now())
}

